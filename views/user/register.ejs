    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.min.js" integrity="sha384-QJHtvGhmr9XOIpI6YVutG+2QOK9T+ZnN4kzFN1RtK3zEFEIsxhlmWl5/YESvpZ13" crossorigin="anonymous"></script>
    <title>Login</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        <script>
        const API = `http://localhost:8001`;
    </script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="http://localhost:8001/public/js/auth.js"></script>
<style>
    .btn-custom-c {
        margin: 5%;
    }
    .force-text-right{
        text-align: left;
    }
</style>
</head>
<body>
    <div class="row mt-5">
    <div class="col-md-3"></div>
    <div class="col-md-6">
    <div class="card text-center">
  <div class="card-header">
    Register
  </div>
  <div class="force-text-right card-body">
<div class="mb-3">
    <label>Name</label>
    <input id="name" class="form-control" type="text" placeholder="Full Name" aria-label="default input example">
</div>
<div class="mb-3">
    <label>Email</label>
    <input id="email" class="form-control" type="email" placeholder="Email " aria-label="default input example">
</div>
   <div class="input-group mb-3">
  <span class="input-group-text" id="basic-addon1">+91</span>
  <input minlength="10" maxlength="10" id="phoneNumber" type="number" class="form-control" placeholder="Phone Number" aria-label="Username" aria-describedby="basic-addon1">
</div>
<div class="mb-3">
    <label>Password</label>
    <input id="password" class="form-control" type="password" placeholder="password" aria-label="default input example">
</div>
<div class="mb-3">
    <label>street</label>
    <input id="street" class="form-control" type="text" placeholder="Street Name" aria-label="default input example">
</div>
<div class="mb-3">
    <label>Pincode</label>
    <input id="pincode" class="form-control" type="number" placeholder="40025" aria-label="default input example">
</div>
  </div>
  <button class="btn btn-primary btn-custom-c" onclick="register()" type="submit">Register</button>
  <div class="card-footer text-muted">
    <a href="https://github.com/karanx-sh">@karanx-sh</a>
  </div>
</div>
</div>
<div class="col-md-3"></div>
</div>
